@model IEnumerable<TrainingWebsite.ViewModels.ViewDetailViewModel>

<table id="dtBasicExample" class="table table-striped table-hover">
    <thead>
        <tr>
            <th>NoiDung</th>
            <th>CreateDate</th>
            <th></th>
        </tr>
    </thead>
    <tbody id="myTable">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.NoiDung
                </td>
                <td>
                    @item.CreateDate
                </td>
                <td>
                    @Html.ActionLink("Download", "DowloadFile", "Dashboard", new { fileName = item.NoiDung })
                </td>
            </tr>

        }
    </tbody>
</table>